<h1>CsvController#show</h1>
<p>Find me in app/views/csv_controller/show.html.erb</p>

<table>
	<thead>
		<tr>
			<th>日付</th>
			<th>始値</th>
			<th>高値</th>
			<th>安値</th>
			<th>終値</th>
			<th>出来高</th>
			<th>売買代金</th>
		</tr>
	</thead>

	<tbody>
		<% @stocks.each do |stock| %>
			<tr>
				<td><%= stock.date %></td>
				<td><%= stock.opening_price %></td>
				<td><%= stock.high_price %></td>
				<td><%= stock.low_price %></td>
				<td><%= stock.closing_price %></td>
				<td><%= stock.turnover_value %></td>
				<td><%= stock.turnover_price %></td>
			</tr>
		<% end %>
	</tbody>
</table>

